<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <OutputType>Library</OutputType>

        <PackageId>CustomLibRollingFileAppenderForNuGet</PackageId>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0</Version>
        <Authors>Elizaveta Korneeva</Authors>
        <Description>Кастомизация библиотеки RollingFileAppender</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RestorePackagesPath>D:\C#\NuGet repository\</RestorePackagesPath>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="log4net" Version="3.0.3" />
    </ItemGroup>

</Project>
